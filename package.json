{
  "name": "@temando/schema-tools",
  "description": "Schema tooling",
  "version": "2.3.0",
  "license": "SEE LICENSE IN LICENCE.md",
  "keywords": [
    "library"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "test": "jest src",
    "test:coverage": "jest src --coverage",
    "coverage": "jest src --coverage",
    "lint": "tslint --project tsconfig.json --type-check",
    "release": "cd build && npm publish",
    "build": "bash -v -e scripts/build",
    "build:link": "yarn build && cd build && yarn link",
    "preversion": "yarn lint && yarn build && yarn test && changelog-verify CHANGELOG.md",
    "version": "version-changelog CHANGELOG.md && git add CHANGELOG.md",
    "postversion": "yarn build"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.12",
    "@types/fs-extra": "^4.0.2",
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.28",
    "changelog-verify": "^1.0.4",
    "jest": "^21.1.0",
    "ts-jest": "^21.0.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-config-temando": "^1.5.0",
    "typescript": "^2.5.2",
    "version-changelog": "^2.1.0"
  },
  "dependencies": {
    "@jdw/jst": "^2.0.0-beta.12",
    "bluebird": "^3.5.0",
    "fs-extra": "^4.0.2",
    "lutils": "^2.4.0",
    "typescript-json-schema": "^0.16.0"
  }
}
